<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.zhipengzhuo.entrytask.widget.ViewPagerTabGroupView
        android:id="@+id/tab_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/setting_view_group_tab_bg">

    </com.example.zhipengzhuo.entrytask.widget.ViewPagerTabGroupView>

    <com.example.zhipengzhuo.entrytask.widget.UnScrollViewPager
        android:id="@+id/viewPage"
        android:layout_above="@id/tab_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.example.zhipengzhuo.entrytask.widget.UnScrollViewPager>

</RelativeLayout>